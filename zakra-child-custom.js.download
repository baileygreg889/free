document.addEventListener('DOMContentLoaded',function(){function elementInViewport(el){let top=el.offsetTop;let left=el.offsetLeft;let width=el.offsetWidth;let height=el.offsetHeight;while(el.offsetParent){el=el.offsetParent;top+=el.offsetTop;left+=el.offsetLeft}
return(top<(window.pageYOffset+window.innerHeight)&&left<(window.pageXOffset+window.innerWidth)&&(top+height)>window.pageYOffset&&(left+width)>window.pageXOffset)}
function getTOCElement(){let element=document.getElementById('ez-toc-container');if(element){return element.offsetTop}}
function cloneElement(){if(typeof tableOfContentStyle!=='undefined'){let element=document.getElementById('ez-toc-container');let clone=element.cloneNode(!0);clone.id='ez-toc-container-cloned';document.body.appendChild(clone);let newclonedElement=document.getElementById('ez-toc-container-cloned');newclonedElement.classList.add(tableOfContentStyle.style);if('narrow-layout'===tableOfContentStyle.style){newclonedElement.innerHTML+='<div class="narrow-drawer-label">'+tableOfContentStyle.label+'</div>';newclonedElement.innerHTML+='<div class="narrow-drawer-toggle-button"></div>';let clickableElement=document.getElementsByClassName('narrow-drawer-toggle-button')[0];clickableElement.onclick=function(){newclonedElement.classList.toggle('open');document.getElementById('page').classList.toggle('push-content')};let hoverableElement=document.getElementById('ez-toc-container-cloned');hoverableElement.onmouseover=function(){newclonedElement.classList.add('hovered')};hoverableElement.onmouseout=function(){newclonedElement.classList.remove('hovered')}}
if('wide-layout'===tableOfContentStyle.style){newclonedElement.innerHTML+='<div class="wide-drawer-close-button"></div>';document.getElementById('wide-drawer-label-toggle').classList.add('show');let clickableElementWide=document.getElementById('wide-drawer-label-toggle');clickableElementWide.onclick=function(){setTimeout(function(){document.getElementById('wide-drawer-label-toggle').classList.add('drag')},100);setTimeout(function(){document.getElementById('ez-toc-container-cloned').classList.add('open')},500)};let clickableElementCloseButton=document.getElementsByClassName('wide-drawer-close-button')[0];clickableElementCloseButton.onclick=function(){setTimeout(function(){document.getElementById('ez-toc-container-cloned').classList.remove('open')},100);setTimeout(function(){document.getElementById('wide-drawer-label-toggle').classList.remove('drag')},500)};let hoverableElement=document.getElementById('ez-toc-container-cloned');hoverableElement.onmouseover=function(){newclonedElement.classList.add('hovered')};hoverableElement.onmouseout=function(){newclonedElement.classList.remove('hovered')}}}}
function init(){let element=document.getElementById('ez-toc-container');let viewPortHeight=window.pageYOffset+window.innerHeight;let clickToggleElement=document.getElementsByClassName('ez-toc-toggle')[0];if(element){clickToggleElement.innerText+='Hide';if(clickToggleElement){clickToggleElement.onclick=function(){element.classList.toggle('toggled');if(clickToggleElement.innerHTML==='Hide'){clickToggleElement.innerHTML='Show'}else{clickToggleElement.innerHTML='Hide'}}}
if(!elementInViewport(element)&&(getTOCElement()<viewPortHeight)){cloneElement()}}}
init();window.onscroll=function(){let element=document.getElementById('ez-toc-container');let viewPortHeight=window.pageYOffset+window.innerHeight;let clonedElement=document.getElementById('ez-toc-container-cloned');if(element){if(!elementInViewport(element)&&(getTOCElement()<viewPortHeight)){if(!clonedElement){cloneElement()}}else{let clonedElement=document.getElementById('ez-toc-container-cloned');let removeClassClone=document.getElementById('wide-drawer-label-toggle');let removePushContent=document.getElementById('page');if(clonedElement){clonedElement.remove()}
if(removeClassClone){removeClassClone.classList.remove('show')}
if(removePushContent){removePushContent.classList.remove('push-content')}}}}});jQuery(document).ready(function($){$('#tg-close').click(function(){$("#tg-site-header-announcement").hide()})})